<%
def set_peer_template_vars(values)
  @name         = values["name"]
  @selector     = values["selector"]
  return
end
-%>
<%- @organizations.each do |organization| -%>
<%- organization.values.first.each do |peer| -%>
<%= set_peer_template_vars(peer.values.first) -%>
apiVersion: v1
kind: Service
metadata:
  name: <%= @selector  %>
  labels:
    run: <%= @selector %>
spec:
  type: ClusterIP 
  selector:
    name: <%= @name %>
  ports:
  - protocol: TCP
    port: 30110
    name: grpc
  - protocol: TCP
    port: 30111
    name: events
  - protocol: TCP
    port: 5984
    name: couchdb
---
<% end -%>
<% end -%>